@model IEnumerable<EsVenerdi.Models.TipoViolazione>

<h1>Tipo Violazione</h1>

<table class="table">
    <thead>
        <tr>
            <th>Descrizione</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Descrizione</td>
            </tr>
        }
    </tbody>
</table>

<a asp-action="Create" class="btn btn-primary">Aggiungi Violazione</a>