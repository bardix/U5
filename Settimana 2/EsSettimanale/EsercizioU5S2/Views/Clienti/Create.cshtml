@model Cliente

<h1>Aggiungi Cliente</h1>
<form asp-action="Create">
    <div class="form-group">
        <label asp-for="CodiceFiscale"></label>
        <input asp-for="CodiceFiscale" class="form-control" />
        <span asp-validation-for="CodiceFiscale" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Cognome"></label>
        <input asp-for="Cognome" class="form-control" />
        <span asp-validation-for="Cognome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Nome"></label>
        <input asp-for="Nome" class="form-control" />
        <span asp-validation-for="Nome" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Città"></label>
        <input asp-for="Città" class="form-control" />
        <span asp-validation-for="Città" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Provincia"></label>
        <input asp-for="Provincia" class="form-control" />
        <span asp-validation-for="Provincia" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" class="form-control" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Telefono"></label>
        <input asp-for="Telefono" class="form-control" />
        <span asp-validation-for="Telefono" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Cellulare"></label>
        <input asp-for="Cellulare" class="form-control" />
        <span asp-validation-for="Cellulare" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Aggiungi</button>
</form>
<a asp-action="Index">Torna alla lista</a>