@model Cliente

<h1>Elimina Cliente</h1>
<div>
    <h4>Sei sicuro di voler eliminare il cliente @Model.Nome @Model.Cognome?</h4>
    <form asp-action="DeleteConfirmed">
        <input type="hidden" asp-for="CodiceFiscale" />
        <button type="submit" class="btn btn-danger">Elimina</button>
        <a asp-action="Index" class="btn btn-secondary">Annulla</a>
    </form>
</div>